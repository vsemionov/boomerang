{
  "name": "boomerang",
  "version": "0.6.0",
  "description": "Boomerang Notes",
  "engines": {
    "node": "6.11.2"
  },
  "scripts": {
    "build": "tsc -p web/src/",
    "build:watch": "tsc -p web/src/ -w",
    "build:e2e": "tsc -p web/e2e/",
    "serve": "lite-server -c=bs-config.json",
    "serve:e2e": "lite-server -c=bs-config.e2e.json",
    "prestart": "npm run build",
    "start": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint web/src/**/*.ts -t verbose",
    "build:aot": "ngc -p tsconfig.json && rollup -c rollup-config.js",
    "postinstall": "npm run build:aot && cpx node_modules/core-js/client/shim.min.js web/static/web/node_modules/core-js/client && cpx node_modules/zone.js/dist/zone.min.js web/static/web/node_modules/zone.js/dist"
  },
  "keywords": [],
  "author": "Victor Semionov <vsemionov@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",

    "angular-in-memory-web-api": "^0.3.2",
    "systemjs": "^0.20.14",
    "core-js": "^2.4.1",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12",

    "angular2-jwt": "^0.2.3",
    "angular2-cookie": "^1.2.6",

    "typescript": "^2.3.4",

    "@angular/compiler-cli": "^4.2.4",
    "@angular/platform-server": "^4.2.4",

    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",

    "cpx": "^1.5.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "lite-server": "^2.3.0",

    "canonical-path": "^0.0.2",
    "tslint": "^5.4.3",
    "lodash": "^4.17.4",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.1.2",
    "rimraf": "^2.6.1",

    "@types/node": "^8.0.3",
    "@types/jasmine": "^2.5.53"
  },
  "repository": {}
}
